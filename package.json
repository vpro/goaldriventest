{
  "name": "goaldriventest",
  "version": "0.0.1",
  "description": "Ask AI to test your website with a specific goal",
  "parserOptions": {
    "sourceType": "module"
  },
  "type": "module",
  "main": "src/goaldriventest.js",
  "bin": {
    "goaldriventest": "src/goaldriventest.js"
  },
  "scripts": {
    "lint": "eslint src",
    "prettier:check": "pnpm prettier --check src package.json",
    "prettier:write": "pnpm prettier --write src package.json"
  },
  "repository": {
    "type": "git",
    "url": ".git"
  },
  "author": "M. Elhorst <m.elhorst@vpro.nl> www.vpro.nl",
  "license": "MIT",
  "files": [
    "*.json",
    "src",
    "examples"
  ],
  "keywords": [
    "test",
    "AI",
    "CLI",
    "automation",
    "goaldriven",
    "goaldriventest",
    "goal-driven-test"
  ],
  "dependencies": {
    "argparse": "^2.0.1",
    "axios": "^1.6.1",
    "luxon": "^3.4.4",
    "openai": "^4.17.4",
    "puppeteer": "^21.5.1",
    "sharp": "^0.32.6"
  },
  "devDependencies": {
    "eslint": "latest",
    "prettier": "latest",
    "eslint-config-prettier": "latest"
  }
}
